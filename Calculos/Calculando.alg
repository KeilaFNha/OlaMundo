algoritmo "CALCULANDO"
// Função :
// Autor :
// Data : 28/07/2021
// Seção de Declarações 
var
   N1, N2, S: INTEIRO
inicio
       escreva ("Informe um número: ")
       leia    (N1)
       escreva ("Informe outro número: ")
       leia    (N2)
      S<- N1 + N2
      escreva ("A soma entre ", N1, " e ", N2, " é igual a ", S)
fimalgoritmo
